{
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    },
    "File": {
      "Path": "shutdown.log",
      "MinLevel": "Debug"
    }
  },
  "ShutdownSettings": {
    "DryRun": true,
    "Volumes": {
      "P:": {
        "CloseHandles": {
          "Enable": false,
          "FlushObjects": true
        },
        "Dismount": {
          "Enable": false
        }
      }
    },
    "VirtualMachines": {
      "Default": {
        "FirstAction": "Shutdown",
        "FirstActionMode": "Soft",
        "SecondAction": "Suspend",
        "SecondActionMode": "Hard"
      },
      "Items": [
        {
          "Order": -1,
          "VmxPath": "E:\\Debian 10.x 64-bit\\TrueNas.vmx"
        }
      ]
    },
    "KillProcesses": {
      "Enable": true,
      "DryRun": false,
      "ida64.exe": {
        "TimeoutSeconds": 60
      }
    }
  }
}